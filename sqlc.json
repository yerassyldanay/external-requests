{
    "version": "1",
    "packages": [
      {
        "name": "taskprovider",
        "path": "./provider/taskprovider",
        "engine": "postgresql",
        "schema": "./connections/postgresconn/migration",
        "queries": "./connections/postgresconn/queries/tasks.sql",
        "emit_json_tags": true,
        "emit_prepared_queries": true,
        "emit_interface": true,
        "emit_exact_table_names": false,
        "emit_empty_slices": false,
        "json_tags_case_style": "snake",
        "output_db_file_name": "db.go",
        "output_models_file_name": "models.go",
        "output_querier_file_name": "querier.go"
      }
    ],
    "overrides": [
      {
        "go_type": {
          "type": "string",
          "pointer": true
        },
        "db_type": "pg_catalog.varchar",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int8",
          "pointer": true
        },
        "db_type": "int2",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int8",
          "pointer": true
        },
        "db_type": "smallint",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int8",
          "pointer": true
        },
        "db_type": "pg_catalog.int2",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int64",
          "pointer": true
        },
        "db_type": "pg_catalog.int8",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int64",
          "pointer": true
        },
        "db_type": "bigint",
        "nullable": true
      },
      {
        "go_type": {
          "type": "int",
          "pointer": true
        },
        "db_type": "pg_catalog.int4",
        "nullable": true
      },
      {
        "go_type": {
          "import": "encoding/json",
          "type": "RawMessage",
          "pointer": true
        },
        "db_type": "json",
        "nullable": true
      }
    ]
  }
  